# Homepage
GET     /                           controllers.ApplicationController.index
GET     /login                      controllers.ApplicationController.login
GET     /logout                     controllers.ApplicationController.logout
GET     /callback                   controllers.Callback.callback(code: Option[String], state: Option[String])
GET     /user                       controllers.UserController.index

PUT /requests controllers.RequestController.update

# An example controller showing a sample home page
GET / controllers.HomeController.index

# Map static resources from the /public folder to the /assets URL path
GET /assets/*file controllers.Assets.versioned(path="/public", file: Asset)
